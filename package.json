{
  "name": "@trustology/trustvault-nodejs-sdk",
  "version": "1.2.1",
  "description": "TrustVault Node.js SDK",
  "main": "./index.js",
  "types": "./index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "npm run clean && tsc && cp package*.json build && cp README.md build",
    "clean": "rm -rf build",
    "quality": "tslint --project . --format stylish --fix",
    "quality-check": "tslint --project . --format stylish",
    "style": "prettier --write 'internal/tests/*.ts' 'src/ts/**/*.ts' *.json",
    "style-check": "prettier --check 'internal/tests/*.ts' 'src/ts/**/*.ts' *.json",
    "test": "mocha --recursive -r ts-node/register './src/ts/**/*.spec.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Trustology/trustvault-nodejs-sdk.git"
  },
  "publishConfig": {
    "@trustology:registry": "https://registry.npmjs.org"
  },
  "author": "Trustology Ltd",
  "license": "MIT",
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-http": "1.5.17",
    "asn1.js": "5.4.1",
    "aws-appsync": "4.0.1",
    "aws-sdk": "2.690.0",
    "bitcoin-address-validation": "1.0.2",
    "bitcoinjs-lib": "5.1.7",
    "bs58check": "2.1.2",
    "elliptic": "6.5.3",
    "ethereumjs-tx": "2.1.2",
    "ethereumjs-util": "7.0.5",
    "graphql": "15.0.0",
    "graphql-tag": "2.10.3",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/dirty-chai": "2.0.2",
    "@types/elliptic": "6.4.12",
    "@types/mocha": "8.0.3",
    "@types/node": "12.12.62",
    "@types/node-fetch": "2.5.7",
    "@types/sinon": "9.0.7",
    "aws-sdk-mock": "5.1.0",
    "chai": "4.2.0",
    "dirty-chai": "2.0.1",
    "mocha": "8.1.3",
    "prettier": "2.0.5",
    "sinon": "9.1.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.9.5"
  }
}
